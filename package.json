  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress --colors",
    "live": "npm run start",
    "build": "npm run build:app && npm run build:sw",
    "build:app": "NODE_ENV=production & webpack --optimize-minimize --config webpack.config.js --progress --profile --colors",
    "build:sw": " sw-precache --config=sw-precache-config.js",
    "lint": "eslint .",
    "test": "karma start --single-run --no-auto-watch karma.config.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.config.js",
    "deploy:github": "git push origin `git subtree split --prefix dist master`:gh-pages --force",
    "lint:fix": "eslint --fix .",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:staged": "lint-staged"
  },
